; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "GuiTransition",
"language": "GDScript",
"path": "res://addons/simple-gui-transitions/transition.gd"
} ]
_global_script_class_icons={
"GuiTransition": "res://addons/simple-gui-transitions/icon.png"
}

[application]

config/name="Simple GUI Transitions"
run/main_scene="res://addons/simple-gui-transitions/example/main.tscn"
config/icon="res://addons/simple-gui-transitions/icon.png"

[autoload]

GuiTransitions="*res://addons/simple-gui-transitions/singleton.gd"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/width=960
window/size/height=540

[editor_plugins]

enabled=PoolStringArray( "res://addons/simple-gui-transitions/plugin.cfg" )

[gui_transitions]

config/default/auto_start=true
config/default/fade_layout=true
config/default/center_pivot=true
config/default/duration=0.5
config/default/delay=0.5
config/default/transition_type="QUAD"
config/default/ease_type="IN_OUT"
config/default/animation_enter=4
config/default/animation_leave=4

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.262745, 0.388235, 0.396078, 1 )
